{
  "size": 10,
  "sort": [{ "@timestamp": { "order": "desc" } }],
  "_source": [
    "@timestamp",
    "timestamp",
    "rule.id",
    "rule.level",
    "rule.description",
    "agent.id",
    "agent.name",
    "decoder.name",
    "location",
    "data.srcip"
  ],
  "query": {
    "bool": {
      "filter": [
        { "range": { "@timestamp": { "gte": "now-1h", "lte": "now" } } }
      ]
    }
  }
}
