{
  "size": 50,
  "sort": [{ "@timestamp": { "order": "asc" } }],
  "_source": [
    "@timestamp",
    "rule.id",
    "rule.level",
    "rule.description",
    "agent.id",
    "agent.name",
    "decoder.name",
    "location",
    "data.win.system.eventID",
    "data.win.eventdata.logonType",
    "data.win.eventdata.targetUserName",
    "data.win.eventdata.subjectUserName"
  ],
  "query": {
    "bool": {
      "filter": [
        { "term": { "agent.id": "004" } },
        {
          "range": {
            "@timestamp": {
              "gte": "CENTER_TIME||-5m",
              "lte": "CENTER_TIME||+5m"
            }
          }
        }
      ]
    }
  }
}
